{
  "name": "prodproject",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 start ./src/bin/www",
    "start-dev": "nodemon -e js,yaml --ignore products.json ./src/bin/www",
    "start-azure": "node ./src/bin/www",
    "start-heroku": "node ./src/bin/www",
    "stop": "pm2 stop ./src/bin/www",
    "delete": "pm2 delete ./src/bin/www",
    "test": "mocha --exit",
    "cover": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "start-virt": "vagrant up",
    "stop-virt": "vagrant halt",
    "rm-virt": "vagrant destroy"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "^1.6.3",
    "jsonschema": "^1.2.4",
    "lowdb": "^1.0.0",
    "morgan": "~1.9.1",
    "request": "^2.88.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pm2": "^4.1.2",
    "supertest": "^4.0.2"
  }
}
